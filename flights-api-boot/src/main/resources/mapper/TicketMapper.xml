<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fh.mapper.TicketMapper">
    <select id="selectTicketByFlightId" parameterType="Integer"  resultType="com.fh.model.Ticket">
        select * from t_ticket where flightId=#{id}
    </select>

</mapper>
